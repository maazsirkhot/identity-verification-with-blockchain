{
	"name": "first-network",
	"version": "1.1",
	"channels": {
		"mainchannel": {
            "orderers": [
                "orderer0",
                "orderer1",
                "orderer2"
            ],
			"peers": [
				"peer0-verify",
				"peer1-verify"
            ],
			"connection": {
				"timeout": {
					"peer": {
						"endorser": "6000",
						"eventHub": "6000",
						"eventReg": "6000"
					}
				}
			}
		}
	},
	"organizations": {
		"verify": {
			"mspid": "verify",
			"adminPrivateKey": {
				"path": "../crypto-config/peerOrganizations/verify/users/Admin@verify/msp/keystore/pvt-cert.pem"
			},
			"peers": ["peer0-verify","peer1-verify"],
			"signedCert": {
				"path": "../crypto-config/peerOrganizations/verify/users/Admin@verify/msp/signcerts/Admin@verify-cert.pem"
			}
		}
    },
    "orderers": {
        "orderer0": {
            "url": "grpcs://orderer0:7050",
            "grpcOptions": {
                "ssl-target-name-override": "orderer0"
            },
            "tlsCACerts": {
                "path": "../crypto-config/ordererOrganizations/orderer/orderers/orderer0/msp/tlscacerts/tlsca.orderer-cert.pem"
            }
        },
        "orderer1": {
            "url": "grpcs://orderer1:7050",
            "grpcOptions": {
                "ssl-target-name-override": "orderer1"
            },
            "tlsCACerts": {
                "path": "../crypto-config/ordererOrganizations/orderer/orderers/orderer1/msp/tlscacerts/tlsca.orderer-cert.pem"
            }
        },
        "orderer2": {
            "url": "grpcs://orderer2:7050",
            "grpcOptions": {
                "ssl-target-name-override": "orderer2"
            },
            "tlsCACerts": {
                "path": "../crypto-config/ordererOrganizations/orderer/orderers/orderer2/msp/tlscacerts/tlsca.orderer-cert.pem"
            }
        }
    },
	"peers": {
		"peer0-verify": {
			"url": "grpcs://peer0-verify:7051",
			"tlsCACerts": {
				"path": "../crypto-config/peerOrganizations/verify/peers/peer0-verify/tls/ca.crt"
			},
            "grpcOptions": {
              "ssl-target-name-override": "peer0-verify"
            }
		},
		"peer1-verify": {
			"url": "grpcs://peer1-verify:8051",
			"tlsCACerts": {
				"path": "../crypto-config/peerOrganizations/verify/peers/peer1-verify/tls/ca.crt"
			},
            "grpcOptions": {
              "ssl-target-name-override": "peer1-verify"
            }
		}
	}
}
