{
	"name": "first-network",
	"version": "1.1",
	"channels": {
		"mainchannel": {
            "orderers": [
                "orderer0"
            ],
			"peers": [
				"peer0-verify"
            ],
			"connection": {
				"timeout": {
					"peer": {
						"endorser": "6000",
						"eventHub": "6000",
						"eventReg": "6000"
					}
				}
			}
		}
	},
	"organizations": {
		"verify": {
			"mspid": "verify",
			"adminPrivateKey": {
				"path": "/tmp/crypto/peerOrganizations/verify/users/Admin@verify/msp/keystore/pvt-cert.pem"
			},
			"peers": ["peer0-verify"],
			"signedCert": {
				"path": "/tmp/crypto/peerOrganizations/verify/users/Admin@verify/msp/signcerts/Admin@verify-cert.pem"
			}
		}
    },
    "orderers": {
        "orderer0": {
            "url": "grpcs://orderer0-service:7050",
            "grpcOptions": {
                "ssl-target-name-override": "orderer0"
            },
            "tlsCACerts": {
                "path": "/tmp/crypto/ordererOrganizations/orderer/orderers/orderer0/msp/tlscacerts/tlsca.orderer-cert.pem"
            }
        }
    },
	"peers": {
		"peer0-verify": {
			"url": "grpcs://peer0-verify-service:7051",
			"tlsCACerts": {
				"path": "/tmp/crypto/peerOrganizations/verify/peers/peer0-verify/tls/ca.crt"
			},
            "grpcOptions": {
              "ssl-target-name-override": "peer0-verify"
            }
		}
	}
}
